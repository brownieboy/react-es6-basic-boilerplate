{
    "name": "react-es6-basic-boilerplate",
    "version": "0.0.1",
    "description": "A very basic boilerplate for using ReactJS with ES6 modules, using Babel as the transpiler and Webpack as the module loader",
    "main": "index.js",
    "scripts": {
        "build": "node_modules/.bin/webpack",
        "buildAll": "gulp cleanBuild && gulp copyJsLibs && gulp copyLocalCSSAndConcat && gulp copyVendorCSSToLocal && npm run build && gulp buildAssets",
        "buildAllAndTest": "npm run buildAll && npm run testBuild",
        "doco": "esdoc -c esdoc.json && gulp openDoco",
        "start": "webpack-dev-server",
        "test": "echo \"Use testDev or testBuild\" && exit 1"
    },
    "keywords": [],
    "author": "Michael Brown <brownieboy@gmail.com>",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "https://github.com/brownieboy/react-es6-basic-boilerplate"
    },
    "devDependencies": {
        "babel-cli": "^6.7.7",
        "babel-core": "^6.7.7",
        "babel-eslint": "^6.0.3",
        "babel-loader": "^6.2.4",
        "babel-preset-es2015": "^6.6.0",
        "babel-preset-react": "^6.5.0",
        "del": "^2.2.0",
        "eslint": "^2.8.0",
        "eslint-config-eslint": "^3.0.0",
        "gulp": "^3.9.1",
        "gulp-clean-css": "^2.0.6",
        "gulp-concat": "^2.6.0",
        "gulp-dom-src": "^0.2.0",
        "gulp-html-replace": "^1.5.5",
        "gulp-inject": "^4.0.0",
        "gulp-print": "^2.0.1",
        "gulp-replace": "^0.5.4",
        "gulp-util": "^3.0.7",
        "open": "0.0.5",
        "react-hot-loader": "^1.3.0",
        "run-sequence": "^1.1.5",
        "webpack": "^1.13.0",
        "webpack-dev-server": "^1.14.1",
        "webpack-merge": "^0.12.0"
    },
    "dependencies": {
        "es6bindall": "0.0.3",
        "react": "^15.0.1",
        "react-dom": "^15.0.1"
    }
}
